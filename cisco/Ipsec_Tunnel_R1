crypto isakmp policy 100
hash sha512
authentication pre-share
group 16
lifetime 3600
enc aes 256
exit
crypto isakmp key 6 cisco add 192.168.1.2                            "R2's tunnel ip address"
crypto ipsec transform-set IPSEC esp-sha512-hmac esp-aes 256
exit
int tunnel 1
ip add 192.168.1.1 255.255.255.0
tunnel source 200.1.1.1                                             "R1's public IP Address"
tunnel dest 200.1.2.1                                               "R2's public IP Address"
tunnel protection ipsec profile IPSEC
exit
do copy running-config startup-config

